{
  "name": "javascript-inviewport",
  "version": "4.0.13",
  "description": "A simple to use, light weight, zero dependency, pure JavaScript and TypeScript ready plugin that uses the intersection observer to determine whether an element has entered within the windows viewport.",
  "main": "./output/inviewport.min.js",
  "types": "./output/inviewport.d.ts",
  "scripts": {
    "tsc": "tsc -p ./tsconfig.json && npm run minify",
    "minify": "uglifyjs --mangle --source-map \"url='./output/inviewport.min.js.map'\" --output ./output/inviewport.min.js -- ./output/inviewport.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianrogren/javascript-inviewport.git"
  },
  "keywords": [
    "inviewport",
    "viewport",
    "js",
    "javascript",
    "typescript",
    "ts",
    "observer",
    "intersection observer"
  ],
  "author": "Ian Rogren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianrogren/javascript-inviewport/issues"
  },
  "homepage": "https://github.com/ianrogren/javascript-inviewport#readme",
  "devDependencies": {
    "typescript": "^4.5.5",
    "uglify-js": "^3.15.0"
  }
}
